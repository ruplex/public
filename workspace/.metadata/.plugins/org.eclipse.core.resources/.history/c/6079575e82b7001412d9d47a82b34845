package test.modules.notice;

import javafx.application.Application;
import javafx.fxml.FXML;
import javafx.fxml.FXMLLoader;
import javafx.scene.Parent;
import javafx.scene.Scene;
import javafx.scene.layout.AnchorPane;
import javafx.scene.text.Text;
import javafx.stage.Stage;
import sun.launcher.resources.launcher;
import test.Main;

public class Error extends Main {
	
	@FXML
	private Text notice_title;
	    
	@FXML
	private Text notice_desc;
	
	public void replace_data_to_interface(String f, String d) {
	
		notice_title.setText(f);
	
		notice_desc.setText(d);
	
	}
	
	public void initialize() throws Exception {
    	
		replace_data_to_interface();
    	
    }
}
